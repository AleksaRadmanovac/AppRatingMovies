{
  "metadata": [
      {
          "src": [
              {
                  "files": [
                      "src/Domen/**/*.csproj",
                      "src/SistemskeOperacije/**/*.csproj"
                  ],
                  "exclude": ["src/Domen/**/obj/**", "src/SistemskeOperacije/**/obj/**"]
              }
          ],
          "dest": "api"
      }
  ],
  "build": {
      "content": [
          {
              "files": [
                  "api/**.yml",
                  "api/**.md",
                  "*.md",
                  "toc.yml"
              ]
          }
      ],
      "globalMetadata": {
          "_appTitle": "Your Documentation Title"
      },
      "template": ["default"]
  }
}